<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Insets?>

<VBox xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" prefWidth="800.0" prefHeight="600.0"
      alignment="CENTER" spacing="10" fx:controller="com.example.MainController">
    <ComboBox fx:id="sceneComboBox" promptText="Sélectionnez une scène">
        <items>
            <FXCollections fx:factory="observableArrayList">
                <String fx:value="Scene 1"/>
                <String fx:value="Scene 2"/>
                <String fx:value="Scene 3"/>
            </FXCollections>
        </items>
    </ComboBox>
    <Button text="Retour" onAction="#onBackButtonClicked"/>
    <StackPane fx:id="sceneContainer" VBox.vgrow="ALWAYS" />
</VBox>

